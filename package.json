{
  "name": "@proximacapital/gulp-tasks",
  "version": "1.0.0",
  "description": "a collection of common gulp tasks",
  "main": "Dist/index",
  "types": "Dist/index.d.ts",
  "files": [
    "Dist/index.js",
    "Dist/index.d.ts",
    "Dist/Src"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "ttsc --build .",
    "clean": "rm -rf Dist",
    "test": "mocha"
  },
  "eslintConfig": {
    "extends": "@proxima-oss/eslint-config"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "mocha": {
    "ui": "qunit",
    "spec": [
      "./Test/**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      "ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/proximacapital/gulp-tasks.git"
  },
  "author": "Developus Maximis <dev@proxima.capital>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/proximacapital/gulp-tasks/issues"
  },
  "homepage": "https://github.com/proximacapital/gulp-tasks#readme",
  "peerDependencies": {
    "ava": "^4.0.0",
    "c8": "7.10.0",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "markdownlint": "0.24.0",
    "markdownlint-cli": "0.30.0"
  },
  "devDependencies": {
    "@proxima-oss/eslint-config": "5.0.0",
    "@types/chai": "4.3.0",
    "@types/gulp": "4.0.9",
    "@types/lodash": "4.14.178",
    "@types/mocha": "9.0.0",
    "chai": "4.3.4",
    "mocha": "9.1.3",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "ttypescript": "1.5.13",
    "typescript": "4.5.2",
    "typescript-transform-paths": "3.3.1"
  }
}
