{
  "name": "@proxima-oss/gulp-tasks",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "version": "1.0.1",
  "engines": {
    "node": "^16.6.1",
    "npm": ">=7.20.0"
  },
  "description": "a collection of common gulp tasks",
  "main": "Dist/Src/index",
  "types": "Dist/Src/index.d.ts",
  "files": [
    "Dist/Src"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "(! command -v gulp && echo 'WARN: MISSING GULP') || gulp check-node",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "clean": "rm -rf Dist",
    "compile": "gulp build",
    "build": "gulp build",
    "lint": "gulp lint",
    "test": "ttsc --build && mocha",
    "coverage": "gulp coverage",
    "start": "node Dist/Src/App.js"
  },
  "eslintConfig": {
    "extends": "@proxima-oss/eslint-config"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "mocha": {
    "ui": "qunit",
    "spec": [
      "./Test/**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "extension": [
      "ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/proximacapital/gulp-tasks.git"
  },
  "author": "Developus Maximis <dev@proxima.capital>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/proximacapital/gulp-tasks/issues"
  },
  "homepage": "https://github.com/proximacapital/gulp-tasks#readme",
  "peerDependencies": {
    "ava": "4.0.0",
    "c8": "7.10.0",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "markdownlint": "0.24.0",
    "markdownlint-cli": "0.30.0"
  },
  "devDependencies": {
    "@proxima-oss/eslint-config": "5.1.0",
    "@types/chai": "4.3.0",
    "@types/gulp": "4.0.9",
    "@types/lodash": "4.14.178",
    "@types/mocha": "9.0.0",
    "chai": "4.3.4",
    "mocha": "9.1.3",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "ttypescript": "1.5.13",
    "typescript": "4.5.2",
    "typescript-transform-paths": "3.3.1"
  },
  "volta": {
    "node": "16.6.1",
    "npm": "7.20.3"
  }
}
